#!/bin/bash

find src -name "*.o" -print0 | xargs -0 rm

# libdhdfs autogen source
rm src/c++/libhdfs/configure
rm src/c++/libhdfs/Makefile.in
rm src/c++/libhdfs/aclocal.m4
rm src/c++/libhdfs/m4/libtool.m4
rm src/c++/libhdfs/m4/ltoptions.m4
rm src/c++/libhdfs/m4/ltsugar.m4
rm src/c++/libhdfs/m4/ltversion.m4
rm src/c++/libhdfs/m4/lt~obsolete.m4
rm -rf src/c++/libhdfs/autom4te.cache/
rm -rf src/c++/libhdfs/config/

# build files
rm -rf build/c++

echo Done
